<div class="col-4">
  <div class="thumbnail">
    <%= link_to(project) do %>
    <img src="http://placehold.it/300x250" alt="" class="img img-polaroid" />
    <% end %>
    <h3 class="thumbnail-header"><%= project.name %></h3>
    <span class="thumbnail-description"><%= project.technologies_used %></span>
    <br>
    <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn" %>
  </div>
</div>
